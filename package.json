  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf dist .vercel node_modules/.cache",
    "setup:dev": "npm install && npm run build",
    "setup:production": "node setup-production.js",
    "deploy:vercel": "npm run build && vercel --prod",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=dist",
    "deploy:cloudflare": "npm run build && wrangler pages publish dist",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:reset": "supabase db reset",
    "supabase:generate-types": "supabase gen types typescript --local > lib/database.types.ts",
    "check:system": "node scripts/check-system.js",
    "check:build": "npm run build && npm run preview"
  },
